{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(git pull:*)",
      "Bash(alias sb)",
      "Bash(alias sb='source ~/.bashrc')",
      "Bash(git remote set-url:*)",
      "Bash(source ~/.bashrc)",
      "Bash(diff:*)",
      "Bash(syncup)",
      "Bash(source:*)",
      "Bash(declare -f gitcommit)",
      "Bash(cp:*)",
      "Bash(echo \"TERM: $TERM\")",
      "Bash(echo \"SHELL: $SHELL\")",
      "Bash(unset:*)",
      "Bash(complete -p)",
      "Bash(shopt)",
      "Bash(ls:*)",
      "Bash(shopt:*)",
      "Bash(echo:*)",
      "Bash(chsh:*)",
      "Bash(/opt/homebrew/bin/bash:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(time:*)",
      "Bash(chmod:*)",
      "Bash(bash:*)",
      "Bash(hostname)",
      "Bash(rm:*)",
      "Bash(/bin/bash:*)",
      "WebFetch(domain:docs.anthropic.com)",
      "Bash(mv:*)",
      "Bash(DISABLE_BASHRC=1 bash -c \"echo ''Bash working without bashrc''\")",
      "Bash(gitc \"Fix bash hanging issue - add terminal detection to prevent infinite exec loop\")",
      "Bash(git add:*)"
    ],
    "deny": []
  }
}